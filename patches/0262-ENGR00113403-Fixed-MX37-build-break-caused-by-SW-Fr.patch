index 69a64f1..a79f73e 100644
--- a/arch/arm/mach-mx37/clock.c
+++ b/arch/arm/mach-mx37/clock.c
@@ -54,6 +54,8 @@ static int cpu_curr_wp;
 static struct cpu_wp *cpu_wp_tbl;
 
 int cpu_wp_nr;
